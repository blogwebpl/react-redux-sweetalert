{
	"name": "react-redux-sweetalert-updated",
	"version": "1.0.2U",
	"description": "Updated SweetAlert with redux-style actions, reducers API",
	"main": "lib/index.js",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf lib dist coverage",
		"lint": "eslint src test",
		"test": "jest",
		"test:watch": "npm test -- --watch",
		"test:cov": "npm test -- --coverage",
		"check": "npm run lint && npm run test",
		"build:lib": "babel src --out-dir lib",
		"build:umd": "webpack src/index.js dist/react-redux-sweetalert.js --config webpack.config.development.js",
		"build:umd:min": "webpack src/index.js dist/react-redux-sweetalert.min.js --config webpack.config.production.js",
		"build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
		"preversion": "npm run clean && npm run check",
		"version": "npm run build",
		"postversion": "git push && git push --tags && npm run clean",
		"prepublish": "npm run clean && npm run build"
	},
	"engines": {
		"node": ">=0.10.0"
	},
	"files": [
		"lib/",
		"index.js"
	],
	"keywords": [
		"react",
		"redux",
		"sweetalert",
		"react-redux"
	],
	"devDependencies": {
		"babel-cli": "^6.22.2",
		"babel-eslint": "^7.1.1",
		"babel-jest": "^18.0.0",
		"babel-loader": "^6.2.10",
		"babel-preset-latest": "^6.22.0",
		"babel-preset-yoctol": "^0.4.0",
		"babel-register": "^6.22.0",
		"enzyme": "^3.11.0",
		"eslint": "^3.14.1",
		"eslint-config-airbnb": "^14.0.0",
		"eslint-config-yoctol": "^0.12.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^3.0.2",
		"eslint-plugin-react": "^6.9.0",
		"jest": "^18.1.0",
		"react": "^18.2.0",
		"react-addons-test-utils": "^15.4.2",
		"react-dom": "^18.2.0",
		"redux": "^3.6.0",
		"redux-immutable": "^3.0.11",
		"rimraf": "^2.5.4",
		"webpack": "^1.14.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"redux": "^2.0.0 || ^3.0.0 || ^4.0.0"
	},
	"browserify": {
		"transform": [
			"loose-envify"
		]
	},
	"dependencies": {
		"compose-function": "^3.0.3",
		"prop-types": "^15.5.10",
		"react-redux": "^5.0.2",
		"sweetalert-react": "^0.4.7",
		"warning": "^3.0.0"
	},
	"optionalDependencies": {
		"immutable": "^3.8.1"
	},
	"jest": {
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/lib/"
		]
	}
}